            };
            var words = hindiText.split(/\s+/);
            for (var i = 0; i < words.length; i++) {
                var word = words[i];
                if (dictionary.hasOwnProperty(word)) {
                    nepaliText += dictionary[word] + " ";
                } else {
                    nepaliText += word + " ";
                }
            }
            document.getElementById('output').innerHTML = nepaliText.trim();
        }
    </script>
</body>
</html>

